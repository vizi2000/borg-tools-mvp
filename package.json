{
  "name": "borg-tools-mvp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "playwright test",
    "test:unit": "vitest",
    "test:coverage": "vitest --coverage",
    "doc:log": "python3 docs/dev-logger.py",
    "doc:milestone": "python3 docs/timeline-manager.py milestone",
    "doc:decision": "python3 docs/decision-tracker.py add",
    "doc:progress": "python3 docs/timeline-manager.py progress",
    "doc:report": "python3 docs/timeline-manager.py report",
    "doc:summary": "python3 docs/timeline-manager.py progress && echo '' && python3 docs/dev-logger.py summary"
  },
  "dependencies": {
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "@trpc/client": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "@trpc/next": "^10.45.0",
    "@tanstack/react-query": "^4.36.1",
    "zod": "^3.22.4",
    "superjson": "^2.2.1",
    "@supabase/supabase-js": "^2.38.4",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/auth-helpers-react": "^0.4.2",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "clsx": "^2.0.0",
    "lucide-react": "^0.294.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/jsonwebtoken": "^9.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "vitest": "^1.0.4",
    "@vitejs/plugin-react": "^4.2.0",
    "playwright": "^1.40.1",
    "@playwright/test": "^1.40.1",
    "@vitest/coverage-v8": "^1.0.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.10.5"
}